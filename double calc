package practice;

public class calculate {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int sum = 0;
		int i;
		int j;
		
		for (i =1; i <=9; i++)
		{
			if (i % 2 != 0)
				continue;
			for (j =1; j <=9; j++)
			{
				System.out.println(i + "X" + j + " = " + i * j);
			}
			System.out.println("\n");
		}
	}

}
